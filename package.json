{
  "name": "nd-microsite",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:dev": "run-p type-check \"build-local {@}\" --",
    "build:stage": "run-p type-check \"build-stage {@}\" --",
    "build:prod": "run-p type-check \"build-prod {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-stage": "vite build --mode stage",
    "build-local": "vite build",
    "build-prod": "vite build --mode prod",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint --fix --ignore-pattern .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@glidejs/glide": "^3.7.1",
    "@types/glidejs__glide": "^3.6.5",
    "@vueuse/core": "^10.11.1",
    "axios": "^1.7.9",
    "vee-validate": "^4.14.7",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "yup": "^1.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^18.19.67",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^6.2.6",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.3.3",
    "vite": "^6.0.3",
    "vue-tsc": "^1.8.27"
  }
}
